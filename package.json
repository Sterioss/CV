    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "node deploy"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "bootstrap": "^4.4.0",
    "core-js": "^3.3.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "lato-font": "^3.0.0",
    "material-icons": "^0.3.1",
    "pikaday": "^1.8.0",
    "popper.js": "^1.16.0",
    "vue": "^2.6.10",
    "vue-ionicons": "^3.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "ftp-deploy": "^2.3.6",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "google",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "html"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
